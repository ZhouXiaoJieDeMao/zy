<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bsoft.quartz.mapper.MyTaskMapper">


    <update id="insertClrcjbxx" parameterType="java.util.Map">
        insert into clrcjbxx(
          last_modified,sfzh,status
          )
        values
        <foreach collection="list" item="item" separator=",">
            (
              current_timestamp,#{item.sfzh},'1'
            )
        </foreach>
    </update>

    <select id="selectBySfzh" resultType="java.util.Map">
        select * from clrcjbxx
        where sfzh in
        <foreach collection="list" item="item" separator="," open="(" close=")">
            #{item.sfzh,jdbcType=VARCHAR}
        </foreach>
    </select>
</mapper>
