<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bsoft.system.mapper.SysDynamicReportMapper">
    
    <resultMap type="SysDynamicReport" id="SysDynamicReportResult">
        <result property="id"    column="id"    />
        <result property="operationSql"    column="operation_sql"    />
        <result property="paramSql"    column="param_sql"    />
        <result property="remark"    column="remark"    />
        <result property="operationName"    column="operation_name"    />
        <result property="operationList"    column="operation_list"    />
        <result property="status"    column="status"    />
        <result property="databaseType"    column="database_type"    />
    </resultMap>

    <sql id="selectSysDynamicReportVo">
        select id, operation_sql, param_sql, remark, operation_name, operation_list, status, database_type from sys_dynamic_report
    </sql>

    <select id="selectSysDynamicReportList" parameterType="SysDynamicReport" resultMap="SysDynamicReportResult">
        <include refid="selectSysDynamicReportVo"/>
        <where>  
            <if test="operationSql != null  and operationSql != ''"> and operation_sql = #{operationSql}</if>
            <if test="paramSql != null  and paramSql != ''"> and param_sql = #{paramSql}</if>
            <if test="operationName != null  and operationName != ''"> and operation_name like concat('%', #{operationName}, '%')</if>
            <if test="operationList != null  and operationList != ''"> and operation_list = #{operationList}</if>
            <if test="status != null  and status != ''"> and status = #{status}</if>
            <if test="databaseType != null  and databaseType != ''"> and database_type = #{databaseType}</if>
        </where>
    </select>
    
    <select id="selectSysDynamicReportById" parameterType="Long" resultMap="SysDynamicReportResult">
        <include refid="selectSysDynamicReportVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertSysDynamicReport" parameterType="SysDynamicReport" useGeneratedKeys="true" keyProperty="id">
        insert into sys_dynamic_report
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="operationSql != null and operationSql != ''">operation_sql,</if>
            <if test="paramSql != null">param_sql,</if>
            <if test="remark != null">remark,</if>
            <if test="operationName != null and operationName != ''">operation_name,</if>
            <if test="operationList != null and operationList != ''">operation_list,</if>
            <if test="status != null and status != ''">status,</if>
            <if test="databaseType != null">database_type,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="operationSql != null and operationSql != ''">#{operationSql},</if>
            <if test="paramSql != null">#{paramSql},</if>
            <if test="remark != null">#{remark},</if>
            <if test="operationName != null and operationName != ''">#{operationName},</if>
            <if test="operationList != null and operationList != ''">#{operationList},</if>
            <if test="status != null and status != ''">#{status},</if>
            <if test="databaseType != null">#{databaseType},</if>
         </trim>
    </insert>

    <update id="updateSysDynamicReport" parameterType="SysDynamicReport">
        update sys_dynamic_report
        <trim prefix="SET" suffixOverrides=",">
            <if test="operationSql != null and operationSql != ''">operation_sql = #{operationSql},</if>
            <if test="paramSql != null">param_sql = #{paramSql},</if>
            <if test="remark != null">remark = #{remark},</if>
            <if test="operationName != null and operationName != ''">operation_name = #{operationName},</if>
            <if test="operationList != null and operationList != ''">operation_list = #{operationList},</if>
            <if test="status != null and status != ''">status = #{status},</if>
            <if test="databaseType != null">database_type = #{databaseType},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteSysDynamicReportById" parameterType="Long">
        delete from sys_dynamic_report where id = #{id}
    </delete>

    <delete id="deleteSysDynamicReportByIds" parameterType="String">
        delete from sys_dynamic_report where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>
